    "@babel/preset-env": "^7.20.2",
    "cac": "^6.7.14",
    "chalk": "^5.1.2",
    "jscodeshift": "^0.14.0",
    "octokit": "^2.0.10",
    "simple-git": "^3.14.1",
    "ts-migrate": "^0.1.34",
    "winston": "^3.8.2"
